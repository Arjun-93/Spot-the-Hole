					<input
						type="text"
						name="imageURL"
						id="imageURL"
						value="<%- pothole -%>"
						hidden
					/>
					<input id="latitude" name="latitude" type="text" hidden />
					<input id="longitude" name="longitude" type="text" hidden />
					<div class="row">
						<div class="col s12">
							<button
								type="submit"
								class="waves-effect waves-light btn-large bg-primary"
							>
								Submit
							</button>
						</div>
					</div>
				</form>
			</div>
