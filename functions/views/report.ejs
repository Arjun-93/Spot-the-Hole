		<div class="container">
			<div class="section">
				<form
					action="/submitReport"
					method="post"
					onsubmit="return validateForm()"
				>
					<div class="editprof-img">
						<div class="">
							<img
								src="<%- pothole -%>"
								alt="Pothole"
								name="image"
							/>
						</div>
					</div>
					<div class="row">
						<div class="input-field col s12">
							<i class="mdi mdi-circle-edit-outline prefix"></i>
							<textarea
								id="description"
								name="description"
								class="materialize-textarea"
								required
							></textarea>
							<label for="description">Brief Description</label>
						</div>
